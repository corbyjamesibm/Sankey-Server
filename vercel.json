{
  "version": 2,
  "name": "targetprocess-d3-visualization-server",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/ws",
      "dest": "/src/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/index.js"
    },
    {
      "src": "/health",
      "dest": "/src/index.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/src/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/index.js": {
      "maxDuration": 30
    }
  }
}