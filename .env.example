# TargetProcess Configuration
TP_DOMAIN=yourcompany.tpondemand.com
TP_USERNAME=your-username
TP_PASSWORD=your-password

# Server Configuration
PORT=3000
NODE_ENV=development

# Optional: Enable debug logging
DEBUG=false

# Optional: Custom cache TTL (in seconds)
CACHE_TTL=300

# Optional: WebSocket heartbeat interval (in milliseconds)
WS_HEARTBEAT_INTERVAL=30000

# Optional: Maximum WebSocket connections
WS_MAX_CONNECTIONS=100